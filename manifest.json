{
  "name": "EOW PWA Demo",
  "short_name": "EOW PWA Demo",
  "start_url": "/",
  "display": "standalone",
  "display_override": ["minimal-ui"],
  "permissions_policy": {
    "window-management": ["self"],
    "fullscreen": ["self"],
    "cross-origin-isolated": ["self"]
  },
  "scope": "/",
  "background_color": "#ffffff",
  "theme_color": "#6b7280",
  "icons": [
    {
      "src": "/icons/logo-eow-icon.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "screenshots": [
  {
    "src": "icons/screenshot.png",
    "sizes": "1280x720",
    "type": "image/png"
  }
]
}
