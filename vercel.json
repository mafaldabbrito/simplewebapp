{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "rewrites": [
    {
      "source": "/dashboard/",
      "destination": "/api/dashboard/index.php"
    },
    {
      "source": "/settings/",
      "destination": "/api/settings/index.php"
    },
    {
      "source": "/",
      "destination": "/api/index.php"
    }
  ]
}
